# Convex deployment URL (run 'npx convex dev' to get this)
NEXT_PUBLIC_CONVEX_URL=

# Clerk authentication keys (get from Clerk dashboard)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Google Gemini API Configuration
GOOGLE_GEMINI_API_KEY=
GOOGLE_GEMINI_MODEL=gemini-1.5-flash
GOOGLE_GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com

# AI Analysis Configuration
AI_ANALYSIS_BATCH_SIZE=10
AI_ANALYSIS_RATE_LIMIT=100
AI_ANALYSIS_TIMEOUT=30000

# Optional: Custom sign-in and sign-up URLs
# NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
# NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
# NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
# NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/dashboard